        "\n",
        "# === 8. Predict User Symptoms === #\n",
        "with open(\"../model/symptom_mapping.json\", \"r\") as f:\n",
        "    synonym_map = json.load(f)\n",
        "\n",
        "def fuzzy_match_symptom(symptom, valid_symptoms, threshold=85):\n",
        "    match = process.extractOne(symptom.lower().strip(), valid_symptoms)\n",
        "    return match[0] if match and match[1] >= threshold else None\n",
        "\n",
        "def predict_user_input(input_text, model, selector, label_encoder, symptoms, synonym_map):\n",
        "    raw_symptoms = [s.strip().lower() for s in input_text.split(\",\")]\n",
        "    mapped_symptoms = [synonym_map.get(sym, sym) for sym in raw_symptoms]\n",
        "    matched = [fuzzy_match_symptom(sym, symptoms) for sym in mapped_symptoms]\n",
        "    matched = [m for m in matched if m]\n",
        "\n",
        "    print(\"✅ Matched Symptoms:\", matched)\n",
        "\n",
        "    input_vec = np.zeros(len(symptoms))\n",
        "    for sym in matched:\n",
        "        idx = symptoms.index(sym)\n",
        "        input_vec[idx] = 1\n",
        "\n",
        "    input_df = pd.DataFrame([input_vec], columns=symptoms)\n",
        "    input_selected = selector.transform(input_df)\n",
        "\n",
        "    probs = model.predict_proba(input_selected)[0]\n",
        "    top_idxs = np.argsort(probs)[::-1][:5]\n",
        "\n",
        "    print(\"\\n🎯 Top Predictions:\")\n",
        "    for idx in top_idxs:\n",
        "        print(f\"🔹 {label_encoder.inverse_transform([idx])[0]} — Confidence: {probs[idx]:.2f}\")\n",
        "\n",
