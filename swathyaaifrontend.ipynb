        "y = df[\"prognosis\"]\n",
        "symptom_columns = X.columns.tolist()\n",
        "\n",
        "# Encode target labels\n",
        "le = LabelEncoder()\n",
        "y_encoded = le.fit_transform(y)\n",
        "\n",
        "# Balance the dataset using undersampling of majority classes\n",
        "df_balanced = pd.DataFrame()\n",
        "min_size = df['prognosis'].value_counts().min()\n",
        "for label in df['prognosis'].unique():\n",
        "    subset = df[df['prognosis'] == label]\n",
        "    subset_downsampled = resample(subset, replace=True, n_samples=min_size, random_state=42)\n",
        "    df_balanced = pd.concat([df_balanced, subset_downsampled])\n",
        "\n",
        "X = df_balanced.drop(\"prognosis\", axis=1)\n",
        "y = df_balanced[\"prognosis\"]\n",
        "y_encoded = le.transform(y)\n",
        "\n",
        "# === 2. Split === #\n",
        "X_train, X_test, y_train, y_test = train_test_split(X, y_encoded, test_size=0.2, stratify=y_encoded, random_state=42)\n",
        "\n",
        "# === 3. Feature Selection === #\n",
        "k_best = SelectKBest(score_func=mutual_info_classif, k=100)\n",
        "X_train_selected = k_best.fit_transform(X_train, y_train)\n",
        "X_test_selected = k_best.transform(X_test)\n",
        "\n",
        "# === 4. Train Logistic Regression with Calibration === #\n",
        "base_model = LogisticRegression(max_iter=1000, class_weight='balanced', solver='liblinear')\n",
        "param_grid = {'C': [0.1, 1.0, 10.0]}\n",
        "grid_search = GridSearchCV(base_model, param_grid, cv=5)\n",
        "grid_search.fit(X_train_selected, y_train)\n",
        "\n",
        "best_lr = grid_search.best_estimator_\n",
        "skf = StratifiedKFold(n_splits=5)\n",
        "calibrated_lr = CalibratedClassifierCV(estimator=best_lr, method='sigmoid', cv=skf)\n",
        "calibrated_lr.fit(X_train_selected, y_train)\n",
