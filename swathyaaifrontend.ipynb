        "!pip install fuzzywuzzy python-Levenshtein\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import json\n",
        "import os\n",
        "from sklearn.naive_bayes import BernoulliNB\n",
        "from sklearn.calibration import CalibratedClassifierCV\n",
        "from sklearn.model_selection import train_test_split, StratifiedKFold, GridSearchCV\n",
        "from sklearn.feature_selection import SelectKBest, mutual_info_classif\n",
        "from sklearn.preprocessing import LabelEncoder\n",
        "from sklearn.metrics import classification_report, accuracy_score\n",
        "import joblib\n",
        "from fuzzywuzzy import process\n",
        "\n",
        "# === 1. Load & Prepare Data === #\n",
        "df = pd.read_csv(\"/content/Disease_dataset.csv\")\n",
        "X = df.drop(\"prognosis\", axis=1)\n",
        "y = df[\"prognosis\"]\n",
        "symptom_columns = X.columns.tolist()\n",
        "\n",
        "# Encode target labels\n",
        "y_encoded = LabelEncoder().fit_transform(y)\n",
        "le = LabelEncoder()\n",
        "le.fit(y)\n",
        "\n",
        "# === 2. Split First (Avoid Overfitting) === #\n",
        "X_train, X_test, y_train, y_test = train_test_split(X, y_encoded, test_size=0.2, stratify=y_encoded, random_state=42)\n",
        "\n",
        "# === 3. Feature Selection === #\n",
        "k_best = SelectKBest(score_func=mutual_info_classif, k=100)\n",
        "X_train_selected = k_best.fit_transform(X_train, y_train)\n",
        "X_test_selected = k_best.transform(X_test)\n",
        "\n",
        "# === 4. Synthetic Data Augmentation === #\n",
        "synthetic_data = [\n",
        "    {\"Disease\": \"Jaundice\", \"Symptoms\": [\"yellowing_of_eyes\", \"fatigue\", \"vomiting\", \"loss_of_appetite\"]},\n",
        "    {\"Disease\": \"Malaria\", \"Symptoms\": [\"fever\", \"chills\", \"headache\", \"nausea\", \"sweating\"]},\n",
        "    {\"Disease\": \"Migraine\", \"Symptoms\": [\"headache\", \"nausea\", \"vomiting\", \"blurred_and_distorted_vision\"]},\n",
        "    {\"Disease\": \"AIDS\", \"Symptoms\": [\"muscle_wasting\", \"weight_loss\", \"high_fever\", \"fatigue\"]},\n",
        "    {\"Disease\": \"Hypothyroidism\", \"Symptoms\": [\"cold_hands_and_feets\", \"weight_gain\", \"abnormal_menstruation\", \"fatigue\"]},\n",
        "    {\"Disease\": \"Heart attack\", \"Symptoms\": [\"chest_pain\", \"sweating\", \"nausea\", \"breathlessness\"]},\n",
        "    {\"Disease\": \"Vertigo\", \"Symptoms\": [\"dizziness\", \"nausea\", \"headache\", \"unsteadiness\"]},\n",
        "    {\"Disease\": \"Paralysis (brain hemorrhage)\", \"Symptoms\": [\"blurred_and_distorted_vision\", \"nausea\", \"fever\", \"weakness_of_one_body_side\"]}\n",
        "]\n",
        "\n",
        "aug_rows, aug_labels = [], []\n",
        "for entry in synthetic_data:\n",
        "    if entry[\"Disease\"] not in le.classes_:\n",
        "        continue  # Skip synthetic diseases not in training classes\n",
        "    row = [0] * len(symptom_columns)\n",
        "    for sym in entry[\"Symptoms\"]:\n",
        "        if sym in symptom_columns:\n",
        "            row[symptom_columns.index(sym)] = 1\n",
        "    aug_rows.append(row)\n",
        "    aug_labels.append(entry[\"Disease\"])\n",
        "\n",
        "synthetic_df = pd.DataFrame(aug_rows, columns=symptom_columns)\n",
        "X_syn_selected = k_best.transform(synthetic_df)\n",
        "\n",
        "X_train_aug = np.vstack([X_train_selected, X_syn_selected])\n",
        "y_train_aug = np.concatenate([y_train, le.transform(aug_labels)])\n",
        "\n",
        "# === 5. Train Naive Bayes with Tuning === #\n",
        "bnb = BernoulliNB()\n",
        "param_grid = {'alpha': [0.1, 0.5, 1.0, 1.5, 2.0]}\n",
        "grid_search = GridSearchCV(bnb, param_grid, cv=5)\n",
        "grid_search.fit(X_train_aug, y_train_aug)\n",
        "\n",
        "best_bnb = grid_search.best_estimator_\n",
        "skf = StratifiedKFold(n_splits=5)\n",
        "calibrated_bnb = CalibratedClassifierCV(estimator=best_bnb, method='sigmoid', cv=skf)\n",
        "calibrated_bnb.fit(X_train_aug, y_train_aug)\n",
        "\n",
        "# === 6. Evaluate on Held-Out Test Set === #\n",
        "y_pred = calibrated_bnb.predict(X_test_selected)\n",
        "print(\"\\n✅ Test Accuracy:\", accuracy_score(y_test, y_pred))\n",
        "print(classification_report(y_test, y_pred))\n",
        "\n",
        "# === 7. Fuzzy Symptom Mapping === #\n",
        "SYMPTOM_SYNONYMS = {\n",
        "    \"fever\": \"high_fever\",\n",
        "    \"mild fever\": \"mild_fever\",\n",
        "    \"high fever\": \"high_fever\",\n",
        "    \"yellow skin\": \"yellowing_of_eyes\",\n",
        "    \"yellow nails\": \"yellowing_of_eyes\",\n",
        "    \"muscle pain\": \"muscle_wasting\",\n",
        "    \"body pain\": \"joint_pain\",\n",
        "    \"cold\": \"chills\",\n",
        "    \"cough\": \"continuous_sneezing\",\n",
        "    \"stomach pain\": \"abdominal_pain\",\n",
        "    \"pain while urinating\": \"painful_urination\",\n",
        "    \"burning sensation\": \"burning_micturition\",\n",
        "    \"loss of eyesight\": \"blurred_and_distorted_vision\",\n",
        "    \"yellowing of skin\": \"yellowing_of_eyes\",\n",
        "    \"yellow eyes\": \"yellowing_of_eyes\"\n",
        "}\n",
        "with open(\"../model/symptom_mapping.json\", \"w\") as f:\n",
        "    json.dump(SYMPTOM_SYNONYMS, f, indent=2)\n",
        "\n",
        "# === 8. Save Model and Feature Selector === #\n",
        "joblib.dump(calibrated_bnb, \"../model/final_model.pkl\")\n",
        "joblib.dump(k_best, \"../model/feature_selector.pkl\")\n",
        "print(\"✅ Model trained, tuned, evaluated, and saved successfully.\")"
      ],
